{
  "name": "dieterlimeback",
  "version": "1.0.0",
  "main": ".eleventy.js",
  "scripts": {
    "build:assets": "webpack --config webpack.config.prod.js",
    "build:site": "ELEVENTY_ENV=production npx eleventy",
    "del:assets": "rimraf ./src/compiled-assets",
    "del:dist": "rimraf ./dist",
    "dev": "npm run dev:assets & npm run dev:site",
    "dev:assets": "webpack --config webpack.config.dev.js",
    "dev:site": "ELEVENTY_ENV=development npx eleventy --serve",
    "prod": "npm-run-all del:dist del:assets build:assets build:site",
    "serve:prod": "serve ./dist/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dlimeb/dieterlimeback.git"
  },
  "author": "Dieter Limeback",
  "license": "",
  "devDependencies": {
    "@11ty/eleventy": "^1.0.2",
    "@11ty/eleventy-img": "^2.0.1",
    "@11ty/eleventy-plugin-rss": "^1.2.0",
    "@babel/core": "7.19.1",
    "@babel/preset-env": "7.19.1",
    "autoprefixer": "10.4.12",
    "babel-loader": "8.2.5",
    "core-js": "3.25.3",
    "css-loader": "6.7.1",
    "eslint": "8.24.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-plugin-import": "2.26.0",
    "fibers": "5.0.3",
    "html-minifier": "4.0.0",
    "md5-file": "5.0.0",
    "mini-css-extract-plugin": "2.6.1",
    "npm-run-all": "4.1.5",
    "optimize-css-assets-webpack-plugin": "^6.0.1",
    "postcss-loader": "7.0.1",
    "rimraf": "3.0.2",
    "sass": "1.55.0",
    "sass-loader": "13.0.2",
    "serve": "^14.0.1",
    "terser-webpack-plugin": "5.3.6",
    "webpack": "^5.74.0",
    "webpack-cli": "4.10.0",
    "webpack-merge": "5.8.0"
  }
}
